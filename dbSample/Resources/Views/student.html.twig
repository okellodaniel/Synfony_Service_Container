<style>
	.table {
		border-collapse: collapse;
	}
	.table th,
	td {
		border-bottom: 1px solid #ddd;
		width: 250px;
		text-align: left;
		align: left;
	}
</style>

<h2>Students database Application</h2>
<table>
	<tr>
		<th>Name</th>
		<th>Address</th>
	</tr>
	{%for x in data%}
		<tr>
			<td>{{x.Name}}</td>
			<td>{{x.Address}}</td>
		</tr>
		{%endfor%}
	</table>
